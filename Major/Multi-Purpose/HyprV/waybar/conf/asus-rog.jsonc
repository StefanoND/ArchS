{
    "custom/power_profile": {
    //shows the current power profile and switches to next on click
    "exec": "~/.apps/waybar/scripts/power_profile",
    "return-type": "json",
    "interval": 20,
    "format": "{}",
    "on-click": "~/.apps/waybar/scripts/power_profile next",
    "signal": 8
    }
}