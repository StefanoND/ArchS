# Wezterm
super + Return
  WAYLAND_DISPLAY=no tdrop -mta wezterm
#  WAYLAND_DISPLAY=wayland-1 tdrop -mta wezterm

# Emacs
super + shift + Return
  WAYLAND_DISPLAY=no tdrop -mta emacsclient -c -a emacs
#  WAYLAND_DISPLAY=wayland-1 tdrop -mta emacsclient -c -a emacs

# Restart swhkd
super + shift + r
  bash -ic restartswhkd

